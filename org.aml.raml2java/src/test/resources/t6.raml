#%RAML 1.0 Library
types:
  Person:
    properties:
      name: string
      lastName: string
      age: number
      hasWife: boolean
  HasKind:
    properties:
      kind: Kind
      name: string
  Manager:
    type: [ Person , HasKind]
    properties:
      manages: 
        type: Person[]
        minItems: 1

  Kind:
    enum: [BASIC,ADVANCED,ELITE]
