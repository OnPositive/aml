#%RAML 1.0 Library
types:
  Person:
    properties:
      name: string
      lastName: string
      age: number
      hasWife: boolean
  HasKind:
    properties:
      kind: Kind
  Manager:
    type: [ Person , HasKind]
    properties:
      manages: Person[]

  Kind:
    enum: [BASIC,ADVANCED,ELITE]
